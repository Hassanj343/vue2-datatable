{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "globals": {
    "$": false
  },
  "extends": [
    "standard",
    "plugin:vue/recommended"
  ],
  "rules": {
    "no-new": 0,
    "no-sequences": 0,
    "no-return-assign": 0,
    "no-mixed-operators": 0,
    "no-trailing-spaces": 0,
    "space-before-function-paren": 0,
    "vue/require-v-for-key": 0,
    "vue/valid-v-for": 0
  }
}
